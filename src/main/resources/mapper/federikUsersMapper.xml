<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.federik.mapper.FederikUsersMapper">
    <select id="selectLoginUser" resultType="Integer" parameterType="com.federik.mapper.dto.UsersDto">
        SELECT COUNT(*) FROM federikusers WHERE net_user_username =#{netUserName}  AND net_user_password =#{netUserPassword}
    </select>
</mapper>